<?php

namespace AppBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Table(name="fos_user")
 * @ORM\Entity(repositoryClass="AppBundle\Repository\UserRepository")
 */
class User extends BaseUser
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var string
     *
     * @ORM\Column(name="givenName", type="text")
     */
    private $givenName;

    /**
     * @var string
     *
     * @ORM\Column(name="familyName", type="text")
     */
    private $familyName;

    /**
     * @var string
     *
     * @ORM\Column(name="address", type="text")
     */
    private $address;

    /**
     * @var string
     *
     * @ORM\Column(name="description", type="text")
     */
    private $description;

    /**
     * @var string
     *
     * @ORM\Column(name="gender", type="text")
     */
    private $gender;

    /**
     * @var integer
     *
     * @ORM\Column(name="height", type="integer", nullable=false)
     */
    private $height;

    /**
     * @var integer
     *
     * @ORM\Column(name="weigth", type="integer", nullable=false)
     */
    private $weigth;

    /**
     * @var string
     *
     * @ORM\Column(name="allergies", type="text")
     */
    private $allergies;

    /**
     * @var integer
     *
     * @ORM\Column(name="work_acitvity_level", type="integer", nullable=false)
     */
    private $workActivityLevel;

    /**
     * @var integer
     *
     * @ORM\Column(name="leisure_activity_level", type="integer", nullable=false)
     */
    private $leisureActivityLevel;

    /**
     * @var string
     *
     * @ORM\Column(name="nutrition_category", type="integer", nullable=false)
     */
    private $nutritionCategory;

    public function __construct()
    {
        parent::__construct();
        // your own logic
    }

    /**
     * Set givenName
     *
     * @param string $givenName
     *
     * @return User
     */
    public function setGivenName($givenName)
    {
        $this->givenName = $givenName;

        return $this;
    }

    /**
     * Get givenName
     *
     * @return string
     */
    public function getGivenName()
    {
        return $this->givenName;
    }

    /**
     * Set familyName
     *
     * @param string $familyName
     *
     * @return User
     */
    public function setFamilyName($familyName)
    {
        $this->familyName = $familyName;

        return $this;
    }

    /**
     * Get familyName
     *
     * @return string
     */
    public function getFamilyName()
    {
        return $this->familyName;
    }

    /**
     * Set address
     *
     * @param string $address
     *
     * @return User
     */
    public function setAddress($address)
    {
        $this->address = $address;

        return $this;
    }

    /**
     * Get address
     *
     * @return string
     */
    public function getAddress()
    {
        return $this->address;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return User
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set gender
     *
     * @param string $gender
     *
     * @return User
     */
    public function setGender($gender)
    {
        $this->gender = $gender;

        return $this;
    }

    /**
     * Get gender
     *
     * @return string
     */
    public function getGender()
    {
        return $this->gender;
    }

    /**
     * Set height
     *
     * @param integer $height
     *
     * @return User
     */
    public function setHeight($height)
    {
        $this->height = $height;

        return $this;
    }

    /**
     * Get height
     *
     * @return integer
     */
    public function getHeight()
    {
        return $this->height;
    }

    /**
     * Set weigth
     *
     * @param integer $weigth
     *
     * @return User
     */
    public function setWeigth($weigth)
    {
        $this->weigth = $weigth;

        return $this;
    }

    /**
     * Get weigth
     *
     * @return integer
     */
    public function getWeigth()
    {
        return $this->weigth;
    }

    /**
     * Set allergies
     *
     * @param string $allergies
     *
     * @return User
     */
    public function setAllergies($allergies)
    {
        $this->allergies = $allergies;

        return $this;
    }

    /**
     * Get allergies
     *
     * @return string
     */
    public function getAllergies()
    {
        return $this->allergies;
    }

    /**
     * Set workAcitvityLevel
     *
     * @param integer $workActivityLevel
     *
     * @return User
     */
    public function setWorkActivityLevel($workActivityLevel)
    {
        $this->workActivityLevel = $workActivityLevel;

        return $this;
    }

    /**
     * Get workAcitvityLevel
     *
     * @return integer
     */
    public function getWorkActivityLevel()
    {
        return $this->workActivityLevel;
    }

    /**
     * Set leasureAcivityLevel
     *
     * @param integer $leisureActivityLevel
     *
     * @return User
     */
    public function setLeisureActivityLevel($leisureActivityLevel)
    {
        $this->leisureActivityLevel = $leisureActivityLevel;

        return $this;
    }

    /**
     * Get leasureAcivityLevel
     *
     * @return integer
     */
    public function getLeisureActivityLevel()
    {
        return $this->leisureActivityLevel;
    }

    /**
     * Set nutritionCategory
     *
     * @param integer $nutritionCategory
     *
     * @return User
     */
    public function setNutritionCategory($nutritionCategory)
    {
        $this->nutritionCategory = $nutritionCategory;

        return $this;
    }

    /**
     * Get nutritionCategory
     *
     * @return integer
     */
    public function getNutritionCategory()
    {
        return $this->nutritionCategory;
    }
}
