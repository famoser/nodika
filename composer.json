{
  "name": "famoser/nodika",
  "description": "emergency service (or any kind of consecutive events) distribution",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "dragonmantank/cron-expression": "^2.0",
    "erusev/parsedown": "^1.6",
    "fzaninotto/faker": "^1.7",
    "ramsey/uuid": "^3",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/maker-bundle": "^1.4",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/property-info": "^4.0",
    "symfony/security-bundle": "^4.0",
    "symfony/serializer": "^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": "^2.6",
    "sensiolabs/security-checker": "^5.0",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "symfony/requirements-checker": "^1.0",
    "deployer/deployer": "^6.1",
    "friendsofphp/php-cs-fixer": "^2.7",
    "symfony/browser-kit": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/debug-bundle": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4.0",
    "symfony/webpack-encore-pack": "^1"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security:check": "symfony-cmd",
      "doctrine:migrations:migrate -q": "symfony-cmd",
      "requirements-checker": "script",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "prepare-environment": [
      "vendor/bin/cghooks update",
      "vendor/bin/requirements-checker",
      "php bin/console security:check",
      "php bin/console doctrine:migrations:migrate -q",
      "php bin/console doctrine:fixtures:load -q"
    ],
    "pre-install-cmd": "@configure-environment",
    "post-install-cmd": "@prepare-environment",
    "pre-update-cmd": "@configure-environment",
    "post-update-cmd": "@prepare-environment"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "projectTitle": "nodika",
    "symfony": {
      "id": "01C1WTEKAQ8E4EGPA14JTQ2WHZ",
      "allow-contrib": false
    },
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "./vendor/bin/php-cs-fixer fix  --config=.php_cs",
        "git update-index --again"
      ],
      "post-merge": "composer install"
    }
  }
}
