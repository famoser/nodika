{
    "name": "famoser/nodika",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "erusev/parsedown": "^1.6",
        "fzaninotto/faker": "^1.7",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "php-coveralls/php-coveralls": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^3",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.7",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^3.4",
        "symfony/requirements-checker": "1.0.3"
    },
    "config": {
        "platform": {
          "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check --end-point=http://security.sensiolabs.org/check_lock": "script",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "projectTitle": "nodika",
        "symfony": {
            "id": "01C1WTEKAQ8E4EGPA14JTQ2WHZ",
            "allow-contrib": false
        }
    }
}
