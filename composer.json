{
  "name": "famoser/nodika",
  "description": "emergency service (or any kind of consecutive events) distribution",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-iconv": "*",
    "ext-pdo_sqlite": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "erusev/parsedown": "^1.6",
    "fzaninotto/faker": "^1.7",
    "javiereguiluz/easyadmin-bundle": "^1.17",
    "php-coveralls/php-coveralls": "^2.0",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "^4.0",
    "symfony/apache-pack": "^1.0",
    "symfony/console": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/requirements-checker": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.0",
    "symfony/webpack-encore-pack": "^1.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "deployer/deployer": "dev-famoser-symfony-4",
    "deployer/phar-update": "dev-famoser-symfony-4",
    "friendsofphp/php-cs-fixer": "^2.7",
    "symfony/browser-kit": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/debug-bundle": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/famoser/deployer"
    },
    {
      "type": "vcs",
      "url": "https://github.com/famoser/phar-update"
    }
  ],
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security:check --end-point=http://security.sensiolabs.org/check_lock": "symfony-cmd",
      "doctrine:migrations:migrate -q": "symfony-cmd",
      "requirements-checker": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "projectTitle": "nodika",
    "symfony": {
      "id": "01C1WTEKAQ8E4EGPA14JTQ2WHZ",
      "allow-contrib": false
    }
  }
}
